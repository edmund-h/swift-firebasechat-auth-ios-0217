# platform :ios, '9.0'

target 'Firechat' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Firebase'
  pod 'FirebaseAuth'

  # Pods for Firechat

  target 'FirechatTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase'
    pod 'FirebaseAuth'

  end

end
